{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://sodaru.com/slp/template/properties",
  "title": "JSON Schema for Properties in Serverless Template by Sodaru Technologies",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^[a-zA-Z0-9]+$": {
      "type": "object",
      "additionalProperties": false,
      "description": "User Parameter",
      "required": ["SAMType"],
      "properties": {
        "SAMType": {
          "enum": ["String", "Number", "List<Number>", "CommaDelimitedList"]
        },
        "schema": {
          "$ref": "https://json-schema.org/draft-07/schema"
        }
      }
    }
  }
}
