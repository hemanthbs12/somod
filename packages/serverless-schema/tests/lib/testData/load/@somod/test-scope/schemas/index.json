{
  "__$schema__": "../../../@somod/serverless-schema/meta-schemas/index.json",
  "$id": "https://json-schema.sodaru.com/@somod/test-scope/schemas/index.json",
  "title": "keys for uri references $schema and $ref are padded with __ to avoid the errors in vscode",
  "type": "object",
  "required": ["Resources"],
  "properties": {
    "Description": {
      "type": "string",
      "maxLength": 256
    },
    "Parameters": {
      "__$ref__": "../../../@somod/serverless-schema/schemas/components/parameters.json"
    },
    "Resources": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^[a-zA-Z0-9]{1,64}$": {
          "oneOf": [{ "$ref": "./ssm-parameter.json" }]
        }
      }
    }
  },
  "additionalProperties": false
}
