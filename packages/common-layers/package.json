{
  "name": "@somod/common-layers",
  "version": "1.4.1",
  "description": "Common Layers for Serverless Functions",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "layers"
  ],
  "homepage": "https://gitlab.com/sodaru/somod/sdk/packages/base-slp-module#readme",
  "author": "Raghavendra K R <raghavendra@sodaru.com>",
  "bugs": {
    "url": "https://gitlab.com/sodaru/somod/sdk/issues"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/sodaru/somod/sdk.git"
  },
  "scripts": {
    "clean": "npx rimraf dist && npx rimraf layers",
    "tsc": "npx tsc",
    "generateLayers": "node ./generateLayers.js",
    "installBaseLayer": "cd ./layers/base/nodejs && npm install",
    "installCustomResourceLayer": "cd ./layers/customResource/nodejs && npm install",
    "installLayers": "npm run installBaseLayer && npm run installCustomResourceLayer",
    "cleanCustomResourceLayer": "node ./cleanLayer.js customResource",
    "cleanLayers": "npm run cleanCustomResourceLayer",
    "build": "npm run clean && npm run tsc && npm run generateLayers && npm run installLayers && npm run cleanLayers",
    "pretest": "$CI || npm run build",
    "test": "echo \"no test specified\""
  },
  "keywords": [
    "slp",
    "somod",
    "sodaru",
    "serverless"
  ],
  "peerDependencies": {
    "@solib/cfn-lambda": "^1.1.1",
    "@solib/common-types-schemas": "^1.0.0",
    "@solib/errors": "^1.1.1",
    "@solib/json-validator": "^1.0.1",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "peerDependenciesMeta": {
    "@solib/cfn-lambda": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/rimraf": "^3.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
