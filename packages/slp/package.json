{
  "name": "@somod/slp",
  "version": "1.4.2",
  "description": "ServerLess Package Builder",
  "keywords": [
    "sodaru",
    "slp",
    "package",
    "builder"
  ],
  "author": "Raghavendra K R <raghavendra@sodaru.com>",
  "homepage": "https://gitlab.com/sodaru/somod/sdk/packages/slp#readme",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "bin": {
    "slp": "./bin/slp.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/sodaru/somod/sdk.git"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "bundle": "npx esbuild src/index.ts --bundle --outdir=dist --platform=node --minify --external:@somod/serverless-schema --external:@somod/common-layers --external:esbuild  --external:@sodev/sodev --external:@typescript-eslint/typescript-estree --external:lodash --external:tslib",
    "build": "npm run clean && npm run bundle",
    "pretest": "$CI || npm run build",
    "test": "echo 'No tests defined'"
  },
  "peerDependencies": {
    "@sodev/sodev": "^1.1.0",
    "aws-sdk": "2.952.0",
    "esbuild": "^0.14.24",
    "typescript": "^4.5.5",
    "@typescript-eslint/typescript-estree": "^4.33.0",
    "@somod/serverless-schema": "^1.4.2",
    "@somod/common-layers": "^1.4.2"
  },
  "devDependencies": {
    "@sodaru/cli-base": "^1.2.0",
    "@types/node": "^17.0.21",
    "typescript": "^4.5.5",
    "@somod/sdk-lib": "^1.4.2",
    "@somod/serverless-schema": "^1.4.2"
  }
}
