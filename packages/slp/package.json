{
  "name": "@somod/slp",
  "version": "1.9.4",
  "description": "ServerLess Package Builder",
  "keywords": [
    "sodaru",
    "slp",
    "package",
    "builder"
  ],
  "author": "Raghavendra K R <raghavendra@sodaru.com>",
  "homepage": "https://gitlab.com/sodaru/somod/sdk/packages/slp#readme",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "bin": {
    "slp": "./bin/slp.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/sodaru/somod/sdk.git"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "bundle": "npx esbuild src/index.ts --bundle --outdir=dist --platform=node --minify --external:@somod/serverless-schema --external:@somod/lambda-base-layer --external:esbuild  --external:@sodev/sodev --external:@typescript-eslint/typescript-estree --external:lodash --external:tslib",
    "build": "npm run clean && npm run bundle",
    "pretest": "$CI || npm run build",
    "test": "echo 'No tests defined'",
    "prepack": "sed -i \"s|OTLP_URL|$OTLP_URL|g\" bin/slp.js && sed -i \"s|OTLP_KEY_NAME|$OTLP_KEY_NAME|g\" bin/slp.js && sed -i \"s|OTLP_KEY_VALUE|$OTLP_KEY_VALUE|g\" bin/slp.js"
  },
  "peerDependencies": {
    "@sodev/sodev": "^1.1.0",
    "@somod/lambda-base-layer": "^1.0.0",
    "@typescript-eslint/typescript-estree": ">=5.22.0",
    "aws-sdk": "2.952.0",
    "esbuild": "^0.14.24",
    "typescript": "^4.5.5",
    "@somod/serverless-schema": "^1.9.4",
    "@somod/parameters-schema": "^1.9.4"
  },
  "devDependencies": {
    "@solib/cli-base": "^1.4.0",
    "@types/node": "^17.0.21",
    "typescript": "^4.5.5",
    "@somod/sdk-lib": "^1.9.4",
    "@somod/serverless-schema": "^1.9.4"
  },
  "dependencies": {
    "@solib/cli-open-telemetry": "^1.0.0"
  }
}
